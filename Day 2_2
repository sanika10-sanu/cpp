#include <iostream>


void swap(int* x, int* y) {
    int temp;    
    temp = *x;   
    *x = *y;     
    *y = temp; 


int main() {
    int a = 28, b = 35;

    std::cout << "Before swapping: a = " << a << ", b = " << b << std::endl;

    
    swap(&a, &b);

    std::cout << "After swapping: a = " << a << ", b = " << b << std::endl;

    return 0;
}
